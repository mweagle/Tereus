{
  "name": "tereus",
  "version": "0.0.1",
  "description": "Tereus CloudFormation Workbench",
  "repository": "https://github.com/mweagle/Tereus",
  "main": "js/app.js",
  "dependencies": {
    "async": "1.4.2",
    "bootstrap": "3.3.5",
    "bootswatch": "3.3.5",
    "browser-storage": "0.0.2",
    "classnames": "2.1.3",
    "flux": "2.1.1",
    "font-awesome": "4.4.0",
    "highlight.js": "8.7.0",
    "jquery": "2.1.4",
    "keymirror": "0.1.1",
    "object-assign": "4.0.1",
    "react": "0.13.3",
    "react-highlight": "0.5.0",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "browserify": "11.0.1",
    "envify": "3.4.0",
    "jest-cli": "0.5.0",
    "reactify": "1.1.1",
    "uglify-js": "2.4.24",
    "watchify": "3.3.1"
  },
  "scripts": {
    "start": "watchify -o js/bundle.js -v -d js/app.js",
    "build": "echo \"Working Dir: $PWD\" && ulimit -n 2560 && ./node_modules/.bin/browserify . -t [envify --NODE_ENV production] | ./node_modules/.bin/uglifyjs -m -c warnings=false > js/bundle.min.js",
    "test": "jest"
  },
  "author": "Matt Weagle",
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  },
  "jest": {
    "rootDir": "./js"
  }
}
